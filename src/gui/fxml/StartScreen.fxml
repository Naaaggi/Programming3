<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="700.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane minWidth="-Infinity" style="-fx-border-color: black;" HBox.hgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="-Infinity" prefWidth="364.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="Infinity" minWidth="-Infinity" prefWidth="236.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="Infinity" minHeight="-Infinity" prefHeight="169.0" vgrow="ALWAYS" />
                <RowConstraints maxHeight="Infinity" minHeight="-Infinity" prefHeight="231.0" vgrow="ALWAYS" />
            </rowConstraints>
            <children>
                <VBox minHeight="-Infinity" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: red;" GridPane.columnIndex="0" GridPane.rowIndex="0">
                    <children>
                        <TextArea fx:id="textarea" minWidth="-Infinity" prefHeight="33.0" prefWidth="411.0" GridPane.fillHeight="true" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                            <padding>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                            </padding>
                        </TextArea>
                        <Label prefHeight="18.0" prefWidth="517.0" text="Drag 2 items here From the showing box to mirror their addresses" />
                        <ListView fx:id="listviewTarget" minHeight="-Infinity" prefHeight="247.0" prefWidth="622.0" GridPane.fillHeight="true" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                            </VBox.margin></ListView>
                        <ListView fx:id="listviewProduzent" minHeight="-Infinity" prefHeight="71.0" prefWidth="613.0" GridPane.fillHeight="true" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <VBox minHeight="-Infinity" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: red;" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="21.0" prefWidth="283.0" text="Hochladen / Produzent Einfügen" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="0.0" prefWidth="283.0" text="Löschen von Selektierter Media" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="0.0" prefWidth="283.0" text="Löschen von Selektierter Produzent" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="19.0" prefWidth="283.0" text="Abrufen von Selektierter Media" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="281.0" text="Auflisten(Mediatyp in textArea eingeben)" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="282.0" text="Nach Access count sortieren" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="280.0" text="Nach Adress sortieren" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="Nach Produnzenten sortieren" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="Save as JBP" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="Load from JBP" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="Save as JOS" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="load from JOS" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="Save selected Media" VBox.vgrow="ALWAYS" />
                        <Button maxHeight="Infinity" maxWidth="Infinity" mnemonicParsing="false" prefHeight="26.0" prefWidth="286.0" text="Load media with adress" VBox.vgrow="ALWAYS" />
                    </children>
                </VBox>
                <PieChart fx:id="piechart" GridPane.columnIndex="1" GridPane.fillHeight="true" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.vgrow="ALWAYS">
                    <opaqueInsets>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </opaqueInsets>
                    <padding>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </padding></PieChart>
                <ListView fx:id="listview" prefHeight="218.0" prefWidth="614.0" GridPane.rowIndex="1" />
            <PointLight color="CHARTREUSE" lightOn="true" GridPane.rowIndex="1" />
            </children>
        </GridPane>
      <PointLight color="CHARTREUSE" lightOn="true" />
    </children>
</HBox>
